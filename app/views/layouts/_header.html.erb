  <div id="desktop-sign-in-sign-up">
    <% if current_user %>
      <p id="sign-in">Welcome, <strong><%= current_user.name.split(' ')[0] %></strong></p>
      <%= link_to "Log out", destroy_user_session_path, data: { turbo_method: :delete }, id: "sign-out" %>
    <% else %>
      <%- if controller_name != 'sessions' %>
        <%= link_to "Log in", new_session_path(resource_name), id: "sign-in" %><br />
      <% end %>
      <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
        <%= link_to "Create Account", new_registration_path(resource_name), id: "sign-up" %><br />
      <% end %>
    <% end %>
  </div>
</header>