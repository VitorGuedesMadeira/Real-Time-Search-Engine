      <nav id="navigation-container">
        <ul>
          <li>
            <%= link_to 'Articles', root_path, class: 'nav-link' %>
          </li>
          <li>
            <%= link_to 'Analytics', analytics_path, class: 'nav-link' %>
          </li>
        </ul>
      </nav>
